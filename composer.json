{
  "name": "sourcebroker/deployer-extended-typo3",
  "description": "Customisation for `sourcebroker/deployer-typo3-*` stack",
  "license": "MIT",
  "keywords": [
    "TYPO3",
    "deployer"
  ],
  "require": {
    "sourcebroker/deployer-typo3-deploy": "^0.10.0 || dev-main",
    "sourcebroker/deployer-typo3-deploy-ci": "^0.11.0 || dev-main",
    "sourcebroker/deployer-typo3-database": "^0.0.8 || dev-main",
    "sourcebroker/deployer-typo3-media": "^0.0.5 || dev-main",
    "sourcebroker/deployer-extended": "^24 || dev-master"
  },
  "authors": [
    {
      "name": "Krystian Szymukowicz",
      "email": "k.szymukowicz@gmail.com"
    }
  ],
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  },
  "config": {
    "sort-packages": true,
    "vendor-dir": ".Build/vendor"
  }
}
